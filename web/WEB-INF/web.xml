<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	 version="3.0">
    <filter>
        <filter-name>SEC FILTER</filter-name>
        <filter-class>net.ezeon.capp.security.SecurityFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>SEC FILTER</filter-name>
        <url-pattern>/edit_view.jsp</url-pattern>
        <url-pattern>/contact_form.jsp</url-pattern>
        <url-pattern>/contact_list_view.jsp</url-pattern>
        <url-pattern>/delete_contact.jsp</url-pattern>
        <url-pattern>/search.jsp</url-pattern>
        <url-pattern>/user_home.jsp</url-pattern>
        <url-pattern>/clist.jsp</url-pattern>
        <url-pattern>/ulist.jsp</url-pattern>
        <url-pattern>/search_user.jsp</url-pattern>
        <url-pattern>/admin_home.jsp</url-pattern>
        <url-pattern>/activate_user.jsp</url-pattern>
        <url-pattern>/delete_user.jsp</url-pattern>
        <url-pattern>/block_user.jsp</url-pattern>
        <url-pattern>/user_list_view.jsp</url-pattern>
    </filter-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Login.jsp</welcome-file>
    </welcome-file-list>
</web-app>
